{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "interface-name": false
  },
  "compilerOptions": {
    "noUnusedLocals": true,
    "removeComments": true,
    "sourceMap": true
  }
}
